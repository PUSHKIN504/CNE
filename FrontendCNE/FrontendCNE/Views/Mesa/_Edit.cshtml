@model CNE.Common.Models.MesaViewModel


<h4>Mesa</h4>
<hr />
<div class="row">
    <form asp-action="Edit">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">

            <input asp-for="Mes_Id" class="form-control" id="id" name="id" type="hidden" />

            <div class="col-md-10">
                <label asp-for="Mes_Jurado" class="control-label"></label>
                <input asp-for="Mes_Jurado" name="Descripcion" class="form-control" />
                <span asp-validation-for="Mes_Jurado" class="text-danger"></span>
            </div>
            <div class="col-md-10">
                <label asp-for="Mes_Custodio1" class="control-label"></label>
                <input asp-for="Mes_Custodio1" name="Descripcion" class="form-control" />
                <span asp-validation-for="Mes_Custodio1" class="text-danger"></span>
            </div>
            <div class="col-md-10">
                <label asp-for="Mes_Custodio2" class="control-label"></label>
                <input asp-for="Mes_Custodio2" name="Descripcion" class="form-control" />
                <span asp-validation-for="Mes_Custodio2" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <input type="submit" value="Guardar" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-secondary">Cancelar</a>


        </div>
    </form>
</div>