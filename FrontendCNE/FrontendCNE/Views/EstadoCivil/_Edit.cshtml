@model FrontendCNE.Models.EstadoCivilViewModel


<h4>EstadoCivil</h4>
<hr />
<div class="row">
    <form asp-action="Edit">
        <div asp-validation-summary="ModelOnly" class="text-danger"></div>
        <div class="row">
            
                <input asp-for="EsC_Id" class="form-control" id="id" name="id" type="hidden" />

            <div class="col-md-10">
                <label asp-for="EsC_Descripcion" class="control-label"></label>
                <input asp-for="EsC_Descripcion" name="Descripcion" class="form-control" />
                <span asp-validation-for="EsC_Descripcion" class="text-danger"></span>
            </div>
        </div>

        <div class="form-group">
            <input type="submit" value="Guardar" class="btn btn-primary" />
            <a asp-action="Index" class="btn btn-secondary">Cancelar</a>


        </div>
    </form>
</div>